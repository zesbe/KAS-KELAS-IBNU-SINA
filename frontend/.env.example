# Supabase Configuration
REACT_APP_SUPABASE_URL=https://xxxxx.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend URL
# For local development: http://localhost:3001
# For production: https://your-backend-domain.railway.app
REACT_APP_BACKEND_URL=http://localhost:3001

# DripSender API (for WhatsApp)
REACT_APP_DRIPSENDER_API_KEY=your_dripsender_api_key

# Pakasir Payment Gateway
REACT_APP_PAKASIR_SLUG=your_pakasir_slug
REACT_APP_PAKASIR_API_KEY=your_pakasir_api_key

# VAPID Keys for Push Notifications
# Generate using: node scripts/generate-vapid-keys.js
REACT_APP_VAPID_PUBLIC_KEY=your_vapid_public_key