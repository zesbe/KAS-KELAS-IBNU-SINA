{
  "functions": {
    "api/webhook-pakasir.js": {
      "maxDuration": 10
    },
    "api/cron-daily.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron-daily",
      "schedule": "0 2 * * *"
    }
  ]
}